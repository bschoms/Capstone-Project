SELECT
  start_station_name,
  COUNTIF(member_casual = 'member') AS member_rides,
  COUNTIF(member_casual = 'casual') AS casual_rides,
  COUNT(*) AS total_rides,
  ROUND(COUNTIF(member_casual = 'member') * 100.0 / COUNT(*), 2) AS percent_member
FROM
  `capstoneprojecct.cyclist_data.fullyear_trips`
WHERE
  start_station_name IS NOT NULL
GROUP BY
  start_station_name
HAVING
  total_rides > 225  -- removing low-volume stations
ORDER BY
  percent_member DESC;
